	<queryString>
		<![CDATA[SELECT
     aluno.`idaluno` AS aluno_idaluno,
     aluno.`nome` AS aluno_nome,
     curso.`nome` AS curso_nome,
     disciplina.`nome` AS disciplina_nome,
     nota.`mac` AS nota_mac,
     nota.`media_final` AS nota_media_final,
     nota.`media_prova` AS nota_media_prova,
     turma.`nome` AS turma_nome,
     universidade.`nome` AS universidade_nome
FROM
     `curso` curso INNER JOIN `aluno` aluno ON curso.`idcurso` = aluno.`curso_idcurso`
     INNER JOIN `disciplina` disciplina ON curso.`idcurso` = disciplina.`curso_idcurso`
     INNER JOIN `turma` turma ON curso.`idcurso` = turma.`curso_idcurso`
     INNER JOIN `universidade` universidade ON curso.`universidade_iduniversidade` = universidade.`iduniversidade`
     AND universidade.`iduniversidade` = turma.`universidade_iduniversidade`
     AND universidade.`iduniversidade` = aluno.`universidade_iduniversidade`
     AND turma.`idturma` = aluno.`turma_idturma`
     INNER JOIN `nota` nota ON aluno.`idaluno` = nota.`aluno_idaluno`
     AND disciplina.`iddisciplina` = nota.`disciplina_iddisciplina` order by aluno.nome]]>
	</queryString>
